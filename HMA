SEPARATEWINDOW=false
INPUTS={ "period,13",
         "priceType,0"}
BUFFERSNAME={"hma"}
BUFFERSCOLOR={ClrGreen}
BUFFERSTYPE={DRAW_LINE}
DESCRIPTION={"Hull Moving Average - priceType can be CLOSE=0 OPEN=1 HIGH=2 LOW=3 MEDIAN=4 TYPICAL=5 WEIGHTED=6"}

function calc()
  hma=HMA(priceType,period) 
end





