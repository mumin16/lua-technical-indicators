SEPARATEWINDOW=true
INPUTS={ "period,14",
          "pricetype,5"}
BUFFERSNAME={"cci"}
BUFFERSCOLOR={ClrGreen}
BUFFERSTYPE={DRAW_LINE}
LEVELS={-100,0,100}
DESCRIPTION={"Commodity Channel Index - pricetype can be CLOSE=0 OPEN=1 HIGH=2 LOW=3 MEDIAN=4 TYPICAL=5 WEIGHTED=6"}

function calc()
  cci=CCI(pricetype,period)
end





